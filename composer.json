{
    "name": "crud/laravel",
    "description": ":description",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "~5"
    },
    "authors": [
        {
            "name": "Auto Engine",
            "email": "dhaval48@gmail.com"
           
        }
    ],
    "keywords": ["Laravel", "crudpack"],    
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "autoengine\\crudpack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "autoengine\\crudpack\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "autoengine\\crudpack\\CrudpackServiceProvider"
            ],
            "aliases": {
                "crudpack": "autoengine\\crudpack\\Facades\\crudpack"
            }
        }
    }
}
